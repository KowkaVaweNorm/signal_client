{
  "name": "axsoft-life",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --b ./tsconfig.app.json && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "preview": "vite preview",
    "prettier:format": "npx prettier . --write",
    "prettier:check": "npx prettier . --check",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest --reporter=html --outputFile=./reports/vitest/index.html",
    "test:reports:view": "npx vite preview --outDir reports",
    "postbuild": "node scripts/copy_build_analyzer.js",
    "sb": "storybook dev -p 6006 --no-open",
    "sb:build": "storybook build",
    "precommit": "npm run build && npm run test:unit && npm run lint && npm run lint:scss && npm run prettier:check"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@reduxjs/toolkit": "^2.2.7",
    "antd": "^5.20.6",
    "chart.js": "^4.4.4",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-redux": "^9.1.2",
    "react-router": "^7.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@conarti/eslint-plugin-feature-sliced": "^1.0.4",
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-interactions": "^8.6.9",
    "@storybook/addon-links": "^8.6.9",
    "@storybook/blocks": "^8.6.9",
    "@storybook/builder-vite": "^8.6.9",
    "@storybook/react": "^8.6.9",
    "@storybook/react-vite": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-grid-layout": "^1.3.5",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.4",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.36.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.11.6",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "msw": "^2.4.11",
    "msw-storybook-addon": "^2.0.3",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.78.0",
    "storybook": "^8.6.9",
    "stylelint": "^16.9.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "stylelint-value-no-unknown-custom-properties": "^6.0.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
